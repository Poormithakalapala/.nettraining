@model IEnumerable<MVCassessment.Models.Customer>


<!DOCTYPE html>
<html>
<head>
    <title>Customers in Germany</title>
</head>
<body>
    <h2>Customers in Germany</h2>
    <table border="1">
        <thead>
            <tr>
                <th>Customer ID</th>
                <th>Company Name</th>
                <th>Contact Name</th>
                <th>City</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var customer in Model)
            {
                <tr>
                    <td>@customer.CustomerID</td>
                    <td>@customer.CompanyName</td>
                    <td>@customer.ContactName</td>
                    <td>@customer.City</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
